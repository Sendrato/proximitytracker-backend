	<message name="__Mongo__">
		<property name="Datasource" type="string" value="proximitytracker" />
		<property name="Collection" type="string" value="InfectedUUID" />
		<property name="MessageName" type="string" value="InfectedUUID" />
		<property name="Multiple" type="boolean" value="true" />
	</message>
	
	<message name="Mongo">
